{
  "name": "ioredis-quota",
  "version": "0.4.0",
  "description": "General-purpose quota management for ioredis",
  "main": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "ava": {
    "files": [
      "./dist/tests/*.js",
      "./dist/tests/**/*.js"
    ],
    "concurrency": 1,
    "failFast": true
  },
  "scripts": {
    "clean": "rm -Rf ./dist",
    "build": "npm run clean; tsc",
    "lint": "tslint 'src/**/*.ts?(x)'",
    "prepublish": "npm run build",
    "test": "npm run build && ava"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/xpepermint/ioredis-quota.git"
  },
  "keywords": [
    "quota",
    "quotas",
    "redis",
    "ioredis",
    "limit",
    "limites",
    "rate",
    "throttle",
    "throttling"
  ],
  "bugs": {
    "url": "https://github.com/xpepermint/ioredis-quota/issues"
  },
  "author": "Kristijan Sedlak (Xpepermint)",
  "license": "MIT",
  "homepage": "https://github.com/xpepermint/ioredis-quota#readme",
  "devDependencies": {
    "@types/moment": "2.13.0",
    "@types/node": "7.0.31",
    "ava": "^0.19.1",
    "es6-sleep": "^2.0.2",
    "ioredis": "^3.1.1",
    "tslint": "5.4.3",
    "typescript": "2.3.4"
  },
  "peerDependencies": {
    "ioredis": "^3.1.1"
  },
  "dependencies": {
    "moment": "^2.18.1"
  }
}
